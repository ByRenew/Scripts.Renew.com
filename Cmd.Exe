local function UZYLQ_fake_script() -- TextLabel.LocalScript 
	local script = Instance.new('LocalScript', TextLabel)

	local TEXT = "Cmd.Exe"
	local TextLabel = script.Parent
	
	for i = 1, #TEXT do
		TextLabel.Text = TEXT:sub(1, i)
		wait(.13)
	end
	
end
coroutine.wrap(UZYLQ_fake_script)()
