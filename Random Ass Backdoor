local function QCDVZYD_fake_script()
	local script = Instance.new('LocalScript', TextBox)

	local textBox = script.Parent
	local UserInputService = game:GetService("UserInputService")
	
	textBox.TextWrapped = true
	textBox.MultiLine = true
	
	local inputConn
	
	local function onInputBegan(input, gameProcessed)
		if input.UserInputType == Enum.UserInputType.Keyboard and input.KeyCode == Enum.KeyCode.Return then
			textBox:ReleaseFocus()
		end
	end
	
	textBox.Focused:Connect(function()
		inputConn = UserInputService.InputBegan:Connect(onInputBegan)
	end)
	
	textBox.FocusLost:Connect(function()
		if inputConn then
			inputConn:Disconnect()
			inputConn = nil
		end
	end)
end
coroutine.wrap(QCDVZYD_fake_script)()

